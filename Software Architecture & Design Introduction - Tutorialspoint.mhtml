html, body { height: 100%; background-color: rgb(238, 238, 238); }

html, body, input, textarea, button { -webkit-font-smoothing: antialiased; text-shadow: rgba(0, 0, 0, 0.004) 1px 1px 1px; }

.sticky { position: fixed; top: 0px; width: 100%; }

.google-right-ads { position: sticky; top: 65px; text-align: center; }

#header { top: 0px; right: 0px; left: 0px; z-index: 101; transition: left 0.2s ease 0s; }

.mui-dropdown__menu { z-index: 101; }

#footer { box-sizing: border-box; height: 160px; margin-left: 0px; transition: margin-left 0.2s ease 0s; }

#privacy-banner { position: fixed; bottom: 0px; left: 0px; padding: 1rem 0.5rem; z-index: 1030; color: rgb(0, 0, 0); font-size: 14px; margin: 0px; width: 100%; display: none; border-top: 2px solid rgb(130, 130, 130); }

#privacy-banner p { margin: 0px; color: rgb(0, 0, 0); text-align: center; }

#privacy-banner a { text-decoration: none; margin: 20px auto 0px; display: block; max-width: 150px; }

#privacy-banner a:hover { text-decoration: underline; }

#banner-learn { color: rgb(0, 0, 0); }

#banner-accept { color: rgb(255, 255, 255); border-radius: 5px; font-size: 17px; background: rgb(80, 80, 80); padding: 1px 22px; }

@media (max-width: 768px) {
  .content img { padding-top: 15px; }
}

@media (min-width: 768px) {
  #privacy-banner { padding: 1.5rem 0.5rem; }
  #privacy-banner a { display: inline-block; margin: 0px 10px; }
}

#privacy-banner h1 { font-size: 22px; color: rgb(33, 33, 33); }

#privacy-banner a { font-size: 17px; background: rgb(80, 80, 80); padding: 1px 22px !important; color: rgb(255, 255, 255) !important; }

#privacy-banner { text-align: center; height: 70px; z-index: 19030; background: rgb(241, 241, 241); }

@media (max-width: 768px) {
  #privacy-banner { height: 150px; width: 99%; }
}

#footer { background-color: rgb(2, 136, 209); color: rgb(255, 255, 255); }

#footer a { color: rgb(255, 255, 255); text-decoration: underline; }

html, body { height: auto; background-color: rgb(238, 238, 238); }

.mui-col-md-3 .space-top, .mui-col-md-2 .space-top { margin-top: 15px; }

.mui-col-md-3 .space-bottom, .mui-col-md-2 .space-bottom { margin-bottom: 7px; }

.center { text-align: center !important; }

.prettyprint { background: rgb(238, 238, 238) !important; border: 1px solid rgb(214, 214, 214) !important; }

pre.prettyprint { padding: 5px; font-size: 0.95em; overflow-x: auto; }

.result { background: rgb(238, 238, 238) !important; border: 1px solid rgb(214, 214, 214) !important; }

pre.result { padding: 5px; font-size: 0.95em; overflow-x: auto; }

a.demo { font-family: "Open Sans", Arial, sans-serif; color: rgb(255, 255, 255); font-size: 13px; padding: 3px 10px; border: 1px solid rgb(214, 214, 214); position: absolute; right: 5px; margin: -6px 17px 0px 0px; }

.mui--text-headline, h2 { font-size: 23px; }

.cover img { width: 100%; margin-top: 15px; }

.tutorial-content p { margin: 0.5em 0.2em 0.6em; padding: 0px; text-align: justify; color: rgb(0, 0, 0); font-size: 15px !important; }

.top-ad-heading { padding-bottom: 5px; padding-left: 10px; text-align: center; }

.tutorial-content { background: rgb(255, 255, 255); margin-top: 15px; margin-bottom: 15px; }

.button-borders { border-top: 1px solid rgb(214, 214, 214); border-bottom: 1px solid rgb(214, 214, 214); padding-top: 10px; }

.google-bottom-ads { padding: 10px; margin-bottom: 20px; text-align: center; }

.mui-btn--small { min-width: 107px; }

.fixed + .content { padding-top: 70px; }

.menu-btn { float: left; padding-left: 0px; padding-right: 4px; color: rgb(0, 0, 0); }

#footer a:link, #footer a:visited, .menu-btn a:link, .menu-btn a:visited { text-decoration: none; padding: 7px; color: rgb(0, 0, 0); }

#footer a:hover, #footer a:active { text-decoration: none; color: rgb(170, 170, 170); }

.menu-btn a:hover, .menu-btn a:active { text-decoration: none; padding: 7px; background: rgb(238, 238, 238); }

.top-menu-item a:hover, .top-menu-item a:active { text-decoration: none; color: rgb(170, 170, 170); }

.pre-btn { float: left; padding-left: 0px; padding-bottom: 10px; color: rgb(0, 0, 0); }

.pre-btn a:link, .pre-btn a:visited { text-decoration: none; padding: 7px; color: rgb(0, 0, 0); }

.pre-btn a:hover, .pre-btn a:active { text-decoration: none; padding: 7px; background: rgb(238, 238, 238); }

.nxt-btn { float: right; padding-left: 0px; padding-bottom: 10px; color: rgb(0, 0, 0); }

.nxt-btn a:link, .nxt-btn a:visited { text-decoration: none; padding: 7px; color: rgb(0, 0, 0); }

.nxt-btn a:hover, .nxt-btn a:active { text-decoration: none; padding: 7px; background: rgb(238, 238, 238); }

.download-btn { padding: 9px; border: 1px solid rgb(214, 214, 214); width: 200px; margin: 0px auto 30px; border-radius: 40px; font-size: 30px; white-space: nowrap; vertical-align: middle; cursor: pointer; user-select: none; background-image: none; }

.download-btn:hover, .download-btn a:hover, .download-btn a:active { text-decoration: none; background: rgb(238, 238, 238); color: rgb(76, 76, 76); }

.middle-btn { float: left; position: relative; left: 25%; color: rgb(0, 0, 0); }

.middle-btn a:link, .middle-btn a:visited { text-decoration: none; padding: 7px; color: rgb(0, 0, 0); }

.middle-btn:hover, .middle-btn a:hover, .middle-btn a:active { text-decoration: none; background: rgb(238, 238, 238); color: rgb(76, 76, 76); }

.print-btn { margin-top: 25px; margin-right: 10px; color: rgb(0, 0, 0); }

.print-btn a:link, .print-btn a:visited { text-decoration: none; padding: 7px; color: rgb(0, 0, 0); }

.print-btn a:hover, .print-btn a:active { text-decoration: none; padding: 7px; background: rgb(238, 238, 238); }

.action-btn { cursor: pointer; float: right; margin-top: 18px; margin-right: -1px; color: rgb(0, 0, 0); text-align: center; padding: 3px; }

.action-btn:hover, .action-btn:active { background: rgb(238, 238, 238); }

.ellipses-icon { color: rgb(148, 146, 146); padding: 3px; font-size: 22px !important; }

.mui-appbar { background: rgb(255, 255, 255); z-index: 9999; }

.sidedrawer-toggle { color: rgb(107, 107, 107); margin-left: 10px; }

.sidedrawer-toggle:hover { color: rgb(170, 170, 170); }

.tp-logo { width: 210px; float: left; padding: 4px; }

.left-menu { float: left; }

.right-menu { float: right; }

.mui-dropdown__menu { left: auto; right: 0px; }

.right-menu .mui-btn { background-color: rgb(57, 124, 63); padding: 0px 13px; }

